{
  "name": "tfjs-playground",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write '{components,lib,pages,__{tests,mocks}__}/**/*.{js,css,html}'"
  },
  "dependencies": {
    "@tensorflow-models/body-pix": "2.0.5",
    "@tensorflow-models/coco-ssd": "2.1.0",
    "@tensorflow-models/posenet": "2.2.1",
    "@tensorflow/tfjs": "2.7.0",
    "@tensorflow/tfjs-vis": "1.4.3",
    "@zeit/next-css": "1.0.1",
    "array-to-image": "1.0.0",
    "classnames": "2.2.6",
    "dat.gui": "0.7.7",
    "dis-gui": "2.1.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "formik": "2.2.5",
    "isomorphic-unfetch": "3.1.0",
    "konva": "7.1.8",
    "next": "10.0.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-konva": "16.8.6",
    "react-no-ssr": "1.1.0",
    "react-player": "2.7.0",
    "react-scripts": "4.0.0",
    "react-spring": "8.0.27",
    "react-webcam": "5.2.2",
    "three": "0.122.0",
    "three-gltf-loader": "1.111.0",
    "use-image": "1.0.6",
    "vega-functions": "5.8.0",
    "victor": "1.1.0"
  },
  "devDependencies": {
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2"
  },
  "lint-staged": {
    "pages/**/*.{js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "lib/**/*.{js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "components/**/*.{js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "static/**/*.{json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
